@model List<NoteModel>

<div class="w-100" id="notes-container">

    @if (Model is not null || Model.Count() != 0)
    {
        foreach (var note in Model)
        {
            <div class="d-flex flex-column border mb-2">

                <div class="">

                </div>

                <div class="p-2 text-white">
                    @note.Content
                </div>

            </div>
        }
    }

</div>
